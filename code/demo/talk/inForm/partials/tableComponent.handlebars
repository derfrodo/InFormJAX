export const {{name}}Table = (props: { 
    items: {{detailsTypeInfo.namedBinding}}[];
    onRowClicked?: (item: {{detailsTypeInfo.namedBinding}}) => Promise<void> | void
}) => {
    return <table style={{#surroundWithCurlyBraces}}{{#surroundWithCurlyBraces}} borderSpacing: 4, {{/surroundWithCurlyBraces}}{{/surroundWithCurlyBraces}}>
    <thead>
        <tr>
        {{#each byOrdering}}
            {{#if detail}}
            <th>{{detail.name}}</th>
            {{/if}}
        {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#surroundWithCurlyBraces}}props.items.map((item, index)=>
            <tr key={{#surroundWithCurlyBraces}}item.id{{/surroundWithCurlyBraces}} onClick={{#surroundWithCurlyBraces}}() => props.onRowClicked && props.onRowClicked(item){{/surroundWithCurlyBraces}}>
                {{#each byOrdering}}
                {{#if detail}}
                {{#findElementByPropertyValueAndRegex detail.mappedBy "key" "CELL$"}}
                    <{{componentType.typeName}}
                        item={{surroundWithCurlyBraces "item"}}
                        name={{#surroundWithCurlyBraces}}"{{../detail.name}}"{{/surroundWithCurlyBraces}}
                        value={{#surroundWithCurlyBraces}}item.{{../detail.name}}{{/surroundWithCurlyBraces}}
                    />
                {{/findElementByPropertyValueAndRegex}}
                {{/if}}
                {{/each}}
            </tr>){{/surroundWithCurlyBraces}}
    </tbody>
    </table>;
}