export const {{name}}Table = (props: { 
    items: {{inputTypeInfo.namedBinding}}[];
    onRowClicked?: (item: {{inputTypeInfo.namedBinding}}) => Promise<void> | void
}) => {
    return <table style={{#surroundWithCurlyBraces}}{{#surroundWithCurlyBraces}} borderSpacing: 4, {{/surroundWithCurlyBraces}}{{/surroundWithCurlyBraces}}>
    <thead>
        <tr>
        {{#each byOrdering}}
            <th>{{input.name}}</th>
        {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#surroundWithCurlyBraces}}props.items.map((item, index)=>
            <tr key={{#surroundWithCurlyBraces}}index{{/surroundWithCurlyBraces}} onClick={{#surroundWithCurlyBraces}}() => props.onRowClicked && props.onRowClicked(item){{/surroundWithCurlyBraces}}>
                {{#each byOrdering}}
                {{#if input}}
                {{#findElementByPropertyValueAndRegex input.mappedBy "key" "CELL$"}}
                    <{{componentType.typeName}}
                        item={{surroundWithCurlyBraces "item"}}
                        name={{#surroundWithCurlyBraces}}"{{../input.name}}"{{/surroundWithCurlyBraces}}
                        value={{#surroundWithCurlyBraces}}item.{{../input.name}}{{/surroundWithCurlyBraces}}
                    />
                {{/findElementByPropertyValueAndRegex}}
                {{/if}}
                {{/each}}
            </tr>){{/surroundWithCurlyBraces}}
    </tbody>
    </table>;
}